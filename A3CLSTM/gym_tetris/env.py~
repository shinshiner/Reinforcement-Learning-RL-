import gym
import gym_tetris

class Tetris():
	def __init__(self, rank):
		self.rank = rank
		self.env = gym.make('Tetris-v0')
		self.obs = None

	def reset(self):
		self.obs = self.env.reset()
		return obs

	def step(self, action):
		self.obs, reward, done, info = self.env.step(action)
		return obs, done, reward, info
